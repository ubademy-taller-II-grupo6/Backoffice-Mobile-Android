{
  "expo": {
    "name": "ApiUbademy",
    "slug": "ApiUbademy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ubademyLogo.png",
    "splash": {
      "image": "./assets/ubademyLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ubademyLogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ubademy.group6",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi":true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/ubademyLogo.png",
          "color": "#FFFFFF",
          "sounds": [
            "./assets/notification-sound.wav",
            "./assets/notification-sound-other.wav"
          ]
        }
      ]
    ]
  }
}
